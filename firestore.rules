rules_version = '2';

service cloud.firestore {
  match /databases/{database}/documents {
    match /photographs/{document=**} {
      allow read: if true;
      allow write: if request.auth != null;
    }

    match /testimonials/{document=**} {
      allow read: if true;
      allow write: if request.auth != null;
    }
  }
}
